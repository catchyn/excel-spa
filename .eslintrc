{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],

  "rules": {
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "indent": "off",
    "spaced-comment": "off",
    "operator-linebreak": "off",
    "arrow-parens": "off"
  }
}
